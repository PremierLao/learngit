<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <style>
       /* 填写样式 */
    </style>
</head>

<body>
    <!-- 填写标签 -->
    <script type="text/javascript">
        // 填写JavaScript
        function endsWithVowel(str) {
            var arr=Array.from(str);
            var len=arr.length-1;
            if (
    arr[len] === 'a' ||
    arr[len] === 'e' ||
    arr[len] === 'i' ||
    arr[len] === 'o' ||
    arr[len] === 'u' ||
    arr[len] === 'A' ||
    arr[len] === 'E' ||
    arr[len] === 'I' ||
    arr[len] === 'O' ||
    arr[len] === 'U'
  ) {
    return true;
  }
  return false;
}
    </script>
</body>

</html>
