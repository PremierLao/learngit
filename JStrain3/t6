<!DOCTYPE html>
<html>
    <head>
        <meta charset=utf-8>
    </head>
    <body>
    	
        <script type="text/javascript">
            const _shouldUpdate = (oldVersion, newVersion) => {
                // 补全代码
                var old=oldVersion.split('.');
                var new1=newVersion.split('.')
                if(old[0]<new1[0]){
                    return true;
                }else if(old[0]>new1[0]){
                    return false;
                }
                else {
                if(old[1]<new1[1]){
                    return true;
                }
                else if(old[1]>new1[1]){
                    return false;
                }
                else{
                    if(old[2]<new1[2]){
                        return true;
                    }
                    else if(old[2]>new1[2]){
                        return false;
                    }
                    else {return false;}

                }
                }
            }
        </script>
    </body>
</html>
