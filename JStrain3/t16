<!DOCTYPE html>
<html>
    <head>
        <meta charset=utf-8>
    </head>
    <body>
    	
        <script type="text/javascript">
        const _rank = array => {
            // 补全代码           
            array.sort(function(a,b){
                let totala=a.chinese+a.math+a.english
                let totalb=b.chinese+b.math+b.english
                return totalb-totala
            })
            return array
        }
        </script>
    </body>
</html>
